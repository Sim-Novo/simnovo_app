<script>
  function updateDateTime() {
    const now = new Date();
    const datetimeSpan = document.getElementById('datetime');
    datetimeSpan.textContent = now.toLocaleString();
  }
  updateDateTime();
  setInterval(updateDateTime, 1000);
</script>